<p id="notice"><%= notice %></p>

<h1>Pprocesses</h1>

<table>
  <thead>
    <tr>
      <th>Process</th>
      <th>Customer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pprocesses.each do |pprocess| %>
      <tr>
        <td><%= pprocess.process %></td>
        <td><%= pprocess.customer %></td>
        <td><%= link_to 'Show', pprocess %></td>
        <td><%= link_to 'Edit', edit_pprocess_path(pprocess) %></td>
        <td><%= link_to 'Destroy', pprocess, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pprocess', new_pprocess_path %>
